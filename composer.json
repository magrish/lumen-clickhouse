{
  "name": "magrish/laravel-clickhouse",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "clickhouse",
    "eloquent",
    "orm"
  ],
  "require": {
    "php": ">=7.1",
    "laravel/lumen-framework": ">=5.6",
    "the-tinderbox/clickhouse-builder": "^1.1"
  },
  "repositories": [
      {
          "url": "git@github.com:the-tinderbox/ClickhouseBuilder.git",
          "type": "git"
      }
  ],
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "mockery/mockery": "^1.1",
    "fzaninotto/faker": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Esazykin\\LaravelClickHouse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Esazykin\\LaravelClickHouse\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --stop-on-failure tests/"
  }
}
